{
  "cli": {
    "version": ">= 10.2.1"
  },
  "build": {
    "staging": {
      "channel": "staging",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      }
    },
    "testing": {
      "channel": "testing",
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": false
      },
      "env": {
        "ENVIRONMENT": "testing"
      }
    },
    "development": {
      "android": {
        "buildType": "apk"
      },
      "developmentClient": true,
      
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "developmentClient": true
    }
  },
  "submit": {
    "production": {}
  }
}
